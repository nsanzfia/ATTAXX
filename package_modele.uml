      <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_v4gAoE5IEeOBXLaT_73OFw" name="getY" specification="_v4cWQE5IEeOBXLaT_73OFw">
        <ownedParameter xmi:id="_v4gnsE5IEeOBXLaT_73OFw" type="_vzHKUE5IEeOBXLaT_73OFw"/>
